class Solution {
    public boolean canConstruct(String ransomNote, String magazine) {
        
        int[] hash = new int[26];// hash arr to store count of characters
        int ransomNoteLen = ransomNote.length();
        for(int i=0;i<ransomNoteLen;++i){
            hash[ransomNote.charAt(i)-'a']++; // store characters count from ransomnote string
        }
        int magazineLen =magazine.length();  
         //decrement count for every character in magazine string
        for(int i=0;i<magazineLen;++i){
            hash[magazine.charAt(i)-'a']--; 
        }
        //if count of any character >0 , then it means that character was not in second string so return false ,else true
        for(int i=0;i<26;++i){
            if(hash[i]>0)
                return false;
        }
        return true;
    }
}
